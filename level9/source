class N //108 octets
{
	private:
		void *ptr;
		char *str;
		int nb;

	public:
		N(int); //constructeur
		setAnnotation(char*)
}

N::void N(int i)
{
	this.ptr = 0x8048848;
	this.nb = i;
}

N::setAnnotation(char *ptr)
{
	memcpy(this.str, ptr, strlen(ptr));
}

